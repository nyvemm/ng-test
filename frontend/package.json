{
  "name": "ng.cash-frontend-challenge",
  "version": "1.0.0",
  "description": "A simple React application to manage accounts and transactions",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettify": "prettier --write src/**/*.{ts,tsx}",
    "up": "docker-compose up -d",
    "down": "docker-compose down && docker rmi -f $(docker images -f \"reference=frontend-app\" -q)",
    "reload": "yarn down && yarn up",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "formik": "^2.2.9",
    "jest": "^29.3.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-select": "^5.6.1",
    "react-toastify": "^9.1.1",
    "react-tooltip": "^4.5.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1",
    "styled-components": "^5.3.6",
    "styled-react-modal": "^3.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.24",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.8",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^5.0.1",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^5.1.26",
    "@types/styled-react-modal": "^1.2.2",
    "@types/yup": "^0.32.0",
    "@vitejs/plugin-react": "^2.2.0",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-fonts": "^0.6.0"
  }
}
